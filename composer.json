{
    "name": "nosto/nosto-magento-extension",
    "description": "Nosto Magento extension",
    "license": "OSL v3.0",
    "type":"magento-module",
    "repositories": [
        {
          "type": "vcs",
          "url": "https://github.com/mridang/magazine.git"
        }
    ],
    "require": {
        "nosto/php-sdk": "1.15.5"
    },
    "require-dev": {
        "mridang/magazine": "dev-master",
        "phing/phing": "2.*"
    },
    "config": {
        "vendor-dir": "lib"
    },
    "extra": {
        "map": [
            ["app/code/community/Nosto/*",  "app/code/community/Nosto/"],
            ["app/design/frontend/base/default/layout/nostotagging.xml", "app/design/frontend/base/default/layout/nostotagging.xml"],
            ["app/design/frontend/base/default/template/nostotagging/*", "app/design/frontend/base/default/template/nostotagging/"],
            ["app/design/adminhtml/default/default/layout/nostotagging.xml", "app/design/adminhtml/default/default/layout/nostotagging.xml"],
            ["app/design/adminhtml/default/default/template/nostotagging/*", "app/design/adminhtml/default/default/template/nostotagging/"],
            ["app/etc/modules/*", "app/etc/modules/"],
            ["app/locale/de_DE/Nosto_Tagging.csv", "app/locale/de_DE/Nosto_Tagging.csv"],
            ["app/locale/es_ES/Nosto_Tagging.csv", "app/locale/es_ES/Nosto_Tagging.csv"],
            ["app/locale/fr_FR/Nosto_Tagging.csv", "app/locale/fr_FR/Nosto_Tagging.csv"],
            ["js/nosto/*", " js/nosto/"],
            ["js/nosto-src/*", "js/nosto-src/"]
        ]
    }
}